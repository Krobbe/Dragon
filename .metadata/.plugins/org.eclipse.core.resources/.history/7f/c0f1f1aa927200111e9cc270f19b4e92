package Player;

import java.util.List;

import Game.Card;

/**
 * An interface for a hand in a poker game. 
 * @author lisastenberg
 *
 */
public interface iHand {

	/**
	 * Removes all cards from a hand.
	 */
	public void discard();
	
	public List<Card> getCards();
	
	public void addCard(Card c);
}
