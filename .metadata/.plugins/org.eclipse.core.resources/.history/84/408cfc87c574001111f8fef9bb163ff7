package test;

import static org.junit.Assert.*;

import model.game.Card;
import model.game.Deck;

import org.junit.Test;

public class DeckTest {

	@Test
	public void testPopCard() {
		Deck d = new Deck();
		Card c1, c2;
		boolean check = true;
		
		for(int i = 0; i < 52; i = i+2) {
			c1 = d.popCard();
			c2 = d.popCard();
			
		}
		
		assertTrue(check);
	}

}
